	<munit:before-suite name="test-mule-proxy-config-test-suiteBefore_Suite" doc:id="84b5c5a0-66b0-4589-9641-6b9696e8e859" >
		<logger level="INFO" doc:name="URL vault" doc:id="0b29edc5-e592-4a6b-8bb4-6b462ba1394e" message="#['URL for vault: ' ++ p('vault.protocol') ++ '://' ++ server.ip ++ ':' ++ p('vault.port')]"/>
