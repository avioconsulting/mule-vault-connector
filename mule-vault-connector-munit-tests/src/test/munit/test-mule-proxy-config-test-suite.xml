		<ee:transform doc:name="payload" doc:id="298ba717-eae7-49bb-97e6-88fa6d3186fd">
				<ee:message>
					<ee:set-payload><![CDATA[%dw 2.0
output application/java
---
{
    "data": {
        "name": "test"
    }
}]]></ee:set-payload>
				</ee:message>
				<ee:variables />
			</ee:transform>
